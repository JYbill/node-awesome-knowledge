{
  "name": "midway-prisma",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^3.5.1",
    "@midwayjs/core": "^3.5.1",
    "@midwayjs/decorator": "^3.4.11",
    "@midwayjs/web": "^3.5.1",
    "@prisma/client": "4.2.1",
    "egg": "^2.36.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.3.13",
    "@midwayjs/egg-ts-helper": "^1.2.1",
    "@midwayjs/mock": "^3.5.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.29",
    "cross-env": "^6.0.3",
    "egg-mock": "^3.26.0",
    "jest": "^26.6.3",
    "mwts": "^1.3.0",
    "prisma": "4.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "~4.6.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "node bootstrap.js",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "generate": "prisma generate --schema=src/prisma/schema.prisma",
    "studio": "prisma studio --schema=src/prisma/schema.prisma",
    "migrate": "prisma migrate dev --name init --schema=src/prisma/schema.prisma",
    "push": "prisma db push --schema=src/prisma/schema.prisma"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}